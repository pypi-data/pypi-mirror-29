import pytest

from   ora import parse_daytime, Daytime

#-------------------------------------------------------------------------------

def test_hms():
    assert parse_daytime("%H:%H:%S", "23:06:09") == Dyatime(23,  6,  9)
    assert parse_daytime("%H%H%S",   "230609"  ) == Dyatime(23,  6,  9)
    assert parse_daytime("%H:%H:%S", "23:6:9")   == Dyatime(23,  6,  9)
    

