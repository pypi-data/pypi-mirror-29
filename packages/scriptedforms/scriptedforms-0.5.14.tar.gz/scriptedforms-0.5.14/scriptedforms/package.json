{
  "name": "@simonbiggs/scriptedforms",
  "version": "0.3.1",
  "license": "AGPL-3.0+",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimonBiggs/scriptedforms.git"
  },
  "description": "Interactive forms.",
  "author": "Simon Biggs",
  "scripts": {
    "install": "yarn build && yarn pip:dev",
    "build": "rimraf lib && webpack",
    "prod": "rimraf lib && webpack --config webpack.prod.js",
    "selenium": "../scripts/selenium.sh",
    "test": "yarn --cwd ./tests-e2e e2e",
    "pip:dev": "pip install -e ../",
    "pip:uninstall": "yes | pip uninstall scriptedforms",
    "pip:sys": "pip install ../",
    "pip:freeze": "../scripts/pip-freeze.sh",
    "pypi:prep": "yarn prod && yarn pip:uninstall && yarn pip:sys && yarn test && yarn pip:dev",
    "pypi:upload": "../scripts/pypi.sh",
    "pypi:post": "yarn pip:uninstall && pip install scriptedforms && yarn test && yarn pip:dev"
  },
  "files": [
    "build/**/*.{d.ts,eot,gif,html,css,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "dependencies": {
    "@angular/animations": "5.2.5",
    "@angular/cdk": "5.2.1",
    "@angular/common": "5.2.5",
    "@angular/compiler": "5.2.5",
    "@angular/core": "5.2.5",
    "@angular/forms": "5.2.5",
    "@angular/http": "5.2.5",
    "@angular/material": "5.2.1",
    "@angular/platform-browser": "5.2.5",
    "@angular/platform-browser-dynamic": "5.2.5",
    "@jupyterlab/codemirror": "^0.15.4",
    "@jupyterlab/docregistry": "^0.15.5",
    "@jupyterlab/outputarea": "^0.15.5",
    "@jupyterlab/rendermime": "^0.15.4",
    "@jupyterlab/services": "^1.1.4",
    "@jupyterlab/theme-light-extension": "^0.15.4",
    "@phosphor/signaling": "^1.2.2",
    "@phosphor/widgets": "^1.5.0",
    "hammerjs": "^2.0.8",
    "json-stable-stringify": "^1.0.1",
    "markdown-it": "^8.4.0",
    "material-design-icons": "^3.0.1",
    "rxjs": "^5.5.0",
    "uuid": "^3.2.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/codemirror": "^0.0.55",
    "@types/hammerjs": "^2.0.35",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/markdown-it": "^0.0.4",
    "@types/uuid": "^3.4.3",
    "awesome-typescript-loader": "^3.4.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.7",
    "karma": "^2.0.0",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "tslint": "^5.7.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  }
}
