    "parsers": "ngomodel.YamlParser, ngomodel.JsonParser",
    "serializers": "ngoproject._vs2017.Vs2017Serializer",
    "version": "",
    "vsGuid": "",
    "authorName": "",
    "authorEmail": "__EmailOrNone__",
    "website": "__UrlOrNone__",
    "githubUrl": "__UrlOrNone__",
    "docUrl": "__UrlOrNone__",
    "githubUsername": "numengo",
    "gitRepo": "git@github.com:{{this.githubUsername}}//{{this.repoName}}.git",
    "repoName": "{{ this.type }}-{{ this.name|lower|replace(' ','-') }}",
    "packageName": "{{ this.name|lower|replace(' ','_')|replace('-','_') }}",
