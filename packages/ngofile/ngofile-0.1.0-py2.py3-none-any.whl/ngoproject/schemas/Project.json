    "tox_ini": "__ExistingPathOrNone__",
    "envVar": "DirProject{{this.name}}Root",
    "fileSrcManager": "ngoproject._file.FileSrcManager",
    "fileDataManager": "ngoproject._file.FileDataManager",
    "fileTestManager": "ngoproject._file.FileTestManager"
}
