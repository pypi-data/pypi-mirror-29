    "@nteract/webpack-configurator": "^1.0.0",
    "jquery": "^3.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-notification-system": "^0.2.16",
    "react-redux": "^5.0.7",
