""" Die medicijnen zijn gif en het toedienen ervan is stafbaar !! """

import os; import sys; sys.path.insert(0, os.getcwd())

#sys.path.insert(0, os.getcwd() + os.sep + "obj")

from obj import Config
from obj.shell import parse_cli, reset, startup

from bot.cli import CLI
from bot.core import start

from koning import __version__, __txt__

startup()

def main():
    cfg = start("KONING")
    cfg.workdir = os.path.expanduser("~/.koning")
    cfg.version = __version__
    bot = CLI()
    bot.walk("koning")
    bot.start()

if __name__ == "__main__":
    try:
        main()
    except KeyboardInterrupt:
        print("")
    reset()
    os._exit(0)
