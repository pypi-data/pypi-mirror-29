{
  "description": "Control your Firefox's tabs from command line",
  "manifest_version": 2,
  "name": "BroTab",
  "version": "1.0.2",
  "applications": {
    "gecko": {
      "id": "firefox_mediator@example.org",
      "strict_min_version": "50.0"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["nativeMessaging", "tabs"]
}
